{
  "name": "e2e-tests",
  "version": "0.0.1",
  "description": "E2E tests for OpenAPI project",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"tests/**/*.ts\""
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": ".",
    "testMatch": ["**/tests/**/*.e2e-spec.ts"],
    "moduleFileExtensions": ["ts", "js", "json"],
    "collectCoverageFrom": ["tests/**/*.ts"],
    "coverageDirectory": "coverage"
  }
}

